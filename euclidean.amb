! Program Euclidean! Finds the greatest common divisor of two integers.

{module} Euclidean

{fun} int a, int b -> Euclid int ! function with type int
{while} b != 0
  {if} a > b
    a - b -> a
  {else}
    b - a -> b
  {/else}
  {/if}
{/while}

{return} a {/return}
{/fun}

! main function
{fun} string args[] -> main int
int a, b   ! The program will prompt you to enter two numbers
in -> a, b -> Euclid -> out
! combined input and output and function call in one line
{/fun}

{/module}
